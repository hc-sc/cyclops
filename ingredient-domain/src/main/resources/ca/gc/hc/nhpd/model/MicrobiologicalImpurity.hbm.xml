<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="MicrobiologicalImpurity" table="biological_impurities">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="bioimpurity_id">
	 		<generator class="sequence">
	        	<param name="sequence">biological_impurities_seq</param>
	        </generator>
    </id>
    <property name="toleranceLevel" type="java.lang.Double" column="bioimpurity_tolerance" not-null="true"/>
    <many-to-one name="toleranceUnits" column="units_id" not-null="true"/>
    <many-to-one name="substance" column="impurity_id" not-null="true"/>
    <property name="creationDate" type="date" column="bioimpurity_creation_date"/>
    <property name="lastUpdateDate" type="date" column="bioimpurity_lastupdate_date"/>
    <many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>