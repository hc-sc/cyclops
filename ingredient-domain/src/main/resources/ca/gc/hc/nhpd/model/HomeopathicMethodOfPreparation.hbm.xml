<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="HomeopathicMethodOfPreparation" table="homeopathic_methofpreps">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="homeomop_id">
	 		<generator class="sequence">
	        	<param name="sequence">homeopathic_methofpreps_SEQ</param>
	        </generator>
		</id>
    
        <property name="minimum" type="integer" column="homeomop_minimum" not-null="false"/>
        <property name="maximum" type="integer" column="homeomop_maximum" not-null="false"/>
        <many-to-one name="homeopathicPreparationType" column="homeopreptype_id" not-null="false"/>
        <many-to-one name="homeopathicFormula" column="homeoform_id" not-null="true"/>

		<property name="creationDate" type="date" column="homeomop_creation_date"/>
		<property name="lastUpdateDate" type="date" column="homeomop_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

