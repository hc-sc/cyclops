<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="HomeopathicGenericText" table="homeopathic_generic_texts">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="homeogentext_id">
	 		<generator class="sequence">
	        	<param name="sequence">homeopathic_generic_texts_SEQ</param>
	        </generator>
		</id>
    
        <property name="textE" type="string" length="500" column="homeogentext_name_eng"/>
        <property name="textF" type="string" length="500" column="homeogentext_name_fr"/>
            
		<property name="creationDate" type="date" column="homeogentext_creation_date"/>
		<property name="lastUpdateDate" type="date" column="homeogentext_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

