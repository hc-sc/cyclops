<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="OrganismTypeGroup" table="organism_type_groups">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="orgtypegrp_id">
	 		<generator class="sequence">
	        	<param name="sequence">organism_type_groups_seq</param>
	        </generator>
		</id>
		<property name="commentE" type="string" length="1000" column="orgtypegrp_comment_eng"/>
		<property name="commentF" type="string" length="1000" column="orgtypegrp_comment_fr"/>
		<property name="countriesOfOriginRelevant" type="yes_no" column="orgtypegrp_country_flag"/>
		<property name="geographicDistributionRelevant" type="yes_no" column="orgtypegrp_geog_flag"/>
		<property name="nameE" type="string" length="100" column="orgtypegrp_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="orgtypegrp_name_fr" not-null="true"/>
		<set name="possiblePartTypes" inverse="true" sort="natural" table="organism_type_grp_orgparttypes">
            <cache usage="read-write"/>
			<key column="orgtypegrp_id"/>
			<many-to-many column="orgparttype_id" unique="true" class="OrganismPartType"/>
		</set>
		<!--
		<property name="creationDate" type="date" column="orgtypegrp_creation_date"/>
		<property name="lastUpdateDate" type="date" column="orgtypegrp_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		-->
	</class>
</hibernate-mapping>
