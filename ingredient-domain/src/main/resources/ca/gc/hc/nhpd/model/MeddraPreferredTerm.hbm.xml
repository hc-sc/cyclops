<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="MeddraPreferredTerm" table="meddra_pref_terms">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="meddrapref_id">
       <generator class="sequence">
                <param name="sequence">meddra_pref_terms_seq</param>
        </generator>
    </id>
    	<property name="code" type="string" length="2" column="meddrapref_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="meddrapref_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="meddrapref_name_fr" not-null="true"/>
		<property name="creationDate" type="date" column="meddrapref_creation_date" not-null="true" />
		<property name="lastUpdateDate" type="date" column="meddrapref_lastupdate_date" not-null="true" />
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>
