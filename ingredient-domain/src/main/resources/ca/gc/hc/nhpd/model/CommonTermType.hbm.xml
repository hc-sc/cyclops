<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="CommonTermType" table="common_term_types">
		<id name="id" type="java.lang.Long" column="commontermtype_id">
	 		<generator class="sequence">
	        	<param name="sequence">common_term_types_seq</param>
	        </generator>
		</id>
		<property name="nameE" type="string" length="100" column="commontermtype_name_eng" not-null="false"/>
		<property name="nameF" type="string" length="100" column="commontermtype_name_fr" not-null="false"/>
		<set name="CommonTerms" inverse="true">
            <key column="commontermtype_id"/>
            <one-to-many class="CommonTerm"/>
        </set>
        
		<property name="creationDate" type="date" column="commontermtype_creation_date"/>
		<property name="lastUpdateDate" type="date" column="commontermtype_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

