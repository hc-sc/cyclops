<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="HomeopathicDilution" table="homeopathic_dilutions">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="homeodilu_id">
	 		<generator class="sequence">
	        	<param name="sequence">homeopathic_dilutions_SEQ</param>
	        </generator>
		</id>
		<property name="code" type="string" length="20" column="homeodilu_code" not-null="true"/>
		<property name="nameE" type="string" length="200" column="homeodilu_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="200" column="homeodilu_name_fr" not-null="true"/>
        <property name="commentE" type="string" length="500" column="homeodilu_comment_eng" not-null="true"/>
        <property name="commentF" type="string" length="500" column="homeodilu_comment_fr" not-null="true"/>
        <property name="descriptionE" type="string" length="250" column="homeodilu_desc_eng" not-null="false"/>
        <property name="descriptionF" type="string" length="250" column="homeodilu_desc_fr" not-null="false"/>
        <property name="dilutedAsString" type="string" length="1" column="homeodilu_diluted" not-null="false"/>
		<property name="ratioToBase" type="java.lang.Double" column="homeodilu_ratiotobase" not-null="false"/>
		
		<property name="creationDate" type="date" column="homeodilu_creation_date"/>
		<property name="lastUpdateDate" type="date" column="homeodilu_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

