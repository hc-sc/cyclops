<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="HomeopathicPreparationType" table="homeopathic_preparation_Types">
        <cache usage="read-write"/>	
		<id name="id" type="java.lang.Long" column="homeopreptype_id">
	 		<generator class="sequence">
	        	<param name="sequence">homeopathic_prep_Types_SEQ</param>
	        </generator>
		</id>
		<property name="code" type="string" length="20" column="homeopreptype_code" not-null="true"/>
		<property name="nameE" type="string" length="200" column="homeopreptype_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="200" column="homeopreptype_name_fr" not-null="true"/>
        <property name="commentE" type="string" length="500" column="homeopreptype_comment_eng" not-null="true"/>
        <property name="commentF" type="string" length="500" column="homeopreptype_comment_fr" not-null="true"/>
		
		<property name="creationDate" type="date" column="homeopreptype_creation_date"/>
		<property name="lastUpdateDate" type="date" column="homeopreptype_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

