<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="TraditionalParadigm" table="trad_paradigms">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="tradparadigm_id">
       <generator class="sequence">
                <param name="sequence">trad_paradigm_seq</param>
        </generator>
    </id>
    	<property name="code" type="string" length="2" column="tradparadigm_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="tradparadigm_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="tradparadigm_name_fr" not-null="true"/>
		<property name="descriptionE" type="string" length="1000" column="tradparadigm_desc_eng"/>
		<property name="descriptionF" type="string" length="1000" column="tradparadigm_desc_fr"/>
		<property name="creationDate" type="date" column="tradparadigm_creation_date" not-null="true" />
		<property name="lastUpdateDate" type="date" column="tradparadigm_lastupdate_date" not-null="true" />
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>
