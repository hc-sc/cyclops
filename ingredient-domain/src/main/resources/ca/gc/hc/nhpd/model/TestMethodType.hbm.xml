<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="TestMethodType" table="test_method_types">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="testmthd_type_id">
	 		<generator class="sequence">
	        	<param name="sequence">test_method_types_seq</param>
	        </generator>
		</id>
		<property name="nameE" type="string" length="100" column="testmthd_type_name_eng" not-null="false"/>
		<property name="nameF" type="string" length="100" column="testmthd_type_name_fr" not-null="false"/>
		
		<property name="creationDate" type="date" column="testmthd_type_creation_date"/>
		<property name="lastUpdateDate" type="date" column="testmthd_type_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>		
	</class>
</hibernate-mapping>

