<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.6//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="DosageRange" table="dosage_ranges">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="dosrge_id">
       <generator class="sequence">
                <param name="sequence">dosage_range_seq</param>
        </generator>
    </id>
        <!-- <cache usage="read-write" /> -->
    <property name="maximumDosage" type="double" column="dosrge_maximum_dosage" />
    <property name="minimumDosage" type="double" column="dosrge_minimum_dosage" />
    <many-to-one name="dosageUnits" column="units_id" />
    <property name="creationDate" type="date" column="dosrge_creation_date"/>
	<property name="lastUpdateDate" type="date" column="dosrge_lastupdate_date"/>
	<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>
