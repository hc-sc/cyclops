<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="RouteOfAdministration" table="administration_routes">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="adminrt_id">
	 		<generator class="sequence">
	        	<param name="sequence">administration_routes_seq</param>
	        </generator>
		</id>
		<property name="code" type="string" length="20" column="adminrt_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="adminrt_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="adminrt_name_fr" not-null="true"/>
		<property name="commentE" type="string" length="1000" column="adminrt_comment_eng"/>
		<property name="commentF" type="string" length="1000" column="adminrt_comment_fr"/>
    	<property name="noFixedLimitsAsString" type="string" column="adminrt_no_fixed_limits" not-null="true"/>
    	<property name="sterileRequiredAsString" type="string" column="adminrt_sterileRequired" not-null="true"/>
		<set name="nonMedicinalPurposes" sort="natural" table="purpose_adminrts">
            <cache usage="read-write"/>
			<key column="adminrt_id"/>
			<many-to-many column="purpose_id" class="NonMedicinalPurpose"/>
		</set>
        <set name="possibleDosageForms" sort="natural" table="dosage_form_admin_routes">
            <cache usage="read-write"/>
            <key column="adminrt_id"/>
            <many-to-many column="dosefrm_id" class="DosageForm"/>
        </set>
		
		<property name="creationDate" type="date" column="adminrt_creation_date"/>
		<property name="lastUpdateDate" type="date" column="adminrt_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
	</class>
</hibernate-mapping>