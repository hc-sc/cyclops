<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="RiskType" table="risk_types">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="risktype_id">
       <generator class="sequence">
                <param name="sequence">risk_types_seq</param>
        </generator>
    </id>
    	<property name="code" type="string" length="50" column="risktype_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="risktype_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="risktype_name_fr" not-null="true"/>
		<property name="descriptionE" type="string" length="1000" column="risktype_desc_eng" not-null="false"/>
		<property name="descriptionF" type="string" length="1000" column="risktype_desc_fr" not-null="false"/>
		<property name="creationDate" type="date" column="risktype_creation_date" not-null="true" />
		<property name="lastUpdateDate" type="date" column="risktype_lastupdate_date" not-null="true" />
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>
