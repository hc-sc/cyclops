<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="ApplicationType" table="application_types">
	
     	<cache usage="read-write"/>
	
		<id name="id" type="java.lang.Long" column="apptype_id">
	 		<generator class="sequence">
	        	<param name="sequence">application_types_seq</param>
	        </generator>
		</id>
		<property name="code" type="string" length="100" column="apptype_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="apptype_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="apptype_name_fr" not-null="true"/>
		
		<property name="creationDate" type="date" column="apptype_creation_date"/>
		<property name="lastUpdateDate" type="date" column="apptype_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
		
	</class>
</hibernate-mapping>

