<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
  
<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="IndicationType" table="indication_types">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="indictype_id">
       <generator class="sequence">
                <param name="sequence">indication_types_seq</param>
        </generator>
    </id>
    	<property name="code" type="string" length="50" column="indictype_code" not-null="true"/>
		<property name="nameE" type="string" length="100" column="indictype_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="indictype_name_fr" not-null="true"/>
		<property name="descriptionE" type="string" length="1000" column="indictype_desc_eng" not-null="false"/>
		<property name="descriptionF" type="string" length="1000" column="indictype_desc_fr" not-null="false"/>
		<property name="creationDate" type="date" column="indictype_creation_date" not-null="true" />
		<property name="lastUpdateDate" type="date" column="indictype_lastupdate_date" not-null="true" />
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
  </class>
</hibernate-mapping>
