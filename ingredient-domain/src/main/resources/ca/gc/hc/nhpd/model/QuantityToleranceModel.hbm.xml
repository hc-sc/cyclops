<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="QuantityToleranceModel" table="quality_tolerance_models">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="qualitymodel_id">
	 		<generator class="sequence">
	        	<param name="sequence">quality_tolerance_models_seq</param>
	        </generator>
    </id>
    <property name="code" type="string" length="20" column="qualitymodel_code" not-null="true"/>
    <property name="descriptionE" type="string" length="1000" column="qualitymodel_desc_eng"/>
    <property name="descriptionF" type="string" length="1000" column="qualitymodel_desc_fr"/>
    <property name="nameE" type="string" length="100" column="qualitymodel_name_eng" not-null="true"/>
    <property name="nameF" type="string" length="100" column="qualitymodel_name_fr" not-null="true"/>
    
    <property name="creationDate" type="date" column="qualitymodel_creation_date"/>
    <property name="lastUpdateDate" type="date" column="qualitymodel_lastupdate_date"/>
    <many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>    
  </class>
</hibernate-mapping>

