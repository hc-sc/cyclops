<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="SubTaxa" table="subtaxa">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="subtaxa_id">
	 		<generator class="sequence">
	        	<param name="sequence">subtaxa_seq</param>
	        </generator>
		</id>
		<property name="name" type="string" length="100" column="subtaxa_name" not-null="true"/>
		<many-to-one name="species"
			column="species_id"
			not-null="true"/>
			
		<property name="creationDate" type="date" column="subtaxa_creation_date"/>
		<property name="lastUpdateDate" type="date" column="subtaxa_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
			
	</class>
</hibernate-mapping>