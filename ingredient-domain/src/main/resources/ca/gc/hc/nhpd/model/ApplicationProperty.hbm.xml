<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
  "-//Hibernate/Hibernate Mapping DTD 3.6//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="ApplicationProperty" table="application_properties">
    <cache usage="read-write"/>
    <id name="key" type="java.lang.String" column="key">
 		<generator class="assigned"/>
    </id>
    <property name="data" type="string" column="data"/>
    <property name="flag" type="java.lang.Double" column="flag"/>    

  </class>
</hibernate-mapping>
