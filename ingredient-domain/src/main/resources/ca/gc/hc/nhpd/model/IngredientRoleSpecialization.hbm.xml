<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="IngredientRoleSpecialization" table="role_specials">
	    <cache usage="read-write"/>
		<id name="id" type="java.lang.Long" column="rolespec_id">
	 		<generator class="sequence">
	        	<param name="sequence">role_specials_seq</param>
	        </generator>
		</id>
		
		<property name="className" type="string" length="100" column="rolespec_class_name" not-null="true"/>
		<property name="nameE" type="string" length="100" column="rolespec_name_eng" not-null="true"/>
		<property name="nameF" type="string" length="100" column="rolespec_name_fr" not-null="true"/>

		<property name="creationDate" type="date" column="rolespec_creation_date"/>
		<property name="lastUpdateDate" type="date" column="rolespec_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>

	</class>
</hibernate-mapping>