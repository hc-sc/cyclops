<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="ca.gc.hc.nhpd.model">
  <class name="Permission" table="permissions">
    <cache usage="read-write"/>
    <id name="id" type="java.lang.Long" column="permission_id">
	 		<generator class="sequence">
	        	<param name="sequence">permissions_seq</param>
	        </generator>
    </id>
    
    <property name="nameE" type="string" length="200" column="permission_name_eng" not-null="true"/>
    <property name="nameF" type="string" length="200" column="permission_name_fr" not-null="true"/>
    <property name="creationDate" type="date" column="permission_creation_date"/>
	<property name="lastUpdateDate" type="date" column="permission_lastupdate_date"/>
	<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
	
  </class>
</hibernate-mapping>