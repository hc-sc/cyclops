<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.6//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping package="ca.gc.hc.nhpd.model">
	<class name="AuditEntry" table="audit_entries">
	    <cache usage="read-write"/>
     	<id name="id" type="java.lang.Long" column="audit_entries_id">
	 		<generator class="sequence">
	        	<param name="sequence">audit_entries_seq</param>
	        </generator>
		</id>
		<property name="nameOfObject" type="string" column="audit_entries_object_name" not-null="true"/>
		<property name="idOfObject" type="long" column="audit_entries_object_id" not-null="true"/>
		<property name="originalValue" type="string" column="audit_entries_original_value" not-null="true"/>
		<property name="newValue" type="string" column="audit_entries_new_value" not-null="true"/>			
		<property name="fieldName" type="string" column="audit_entries_field_name" not-null="true"/>			
		<property name="transType" type="string" column="audit_entries_trans_type" not-null="true"/>			
		
		<property name="creationDate" type="date" column="audit_entries_creation_date"/>
		<property name="lastUpdateDate" type="date" column="audit_entries_lastupdate_date"/>
		<many-to-one name="lastUpdateAccount" column="useracc_id" not-null="true"/>
	</class>
</hibernate-mapping>
